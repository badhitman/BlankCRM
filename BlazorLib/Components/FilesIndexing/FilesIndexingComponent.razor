@using MudBlazor
<style>
    .mud-tab-panel {
        overflow: auto;
    }
</style>
@switch (Path.GetExtension(FileObject.FileName))
{
    case ".xlsx":
        <MudExpansionPanels>
            <MudExpansionPanel Text="View">
                <FilesIndexingXLSX FileId="FileObject.Id" />
            </MudExpansionPanel>
        </MudExpansionPanels>
        break;
    case ".docx":
        <MudExpansionPanels>
            <MudExpansionPanel Text="View">
                <FilesIndexingDOCX FileId="FileObject.Id" />
            </MudExpansionPanel>
        </MudExpansionPanels>
        break;
}