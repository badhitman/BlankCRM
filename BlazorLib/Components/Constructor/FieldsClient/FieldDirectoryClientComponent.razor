@inherits FieldComponentBaseModel

<MudSelect Disabled="@(SessionDocument is null)" T="EntryStandardModel" Label="@Field.Name" MultiSelection="@Field.IsMultiSelect" @bind-Value="detect_value" @bind-SelectedValues="options" HelperText="@Field.Hint" ToStringFunc="@converter" ReadOnly="IsBusyProgress">
    @if (DirectoryObject?.Childs.Any() == true)
    {        
        @foreach (EntryStandardModel d in DirectoryObject.Childs)
        {
            <MudSelectItem T="EntryStandardModel" Value="@d">@d.Name</MudSelectItem>
        }
    }
</MudSelect>