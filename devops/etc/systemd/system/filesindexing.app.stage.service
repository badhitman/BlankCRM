[Unit]
Description=filesindexing.app.stage.service

[Service]
WorkingDirectory=/srv/services.stage/FilesIndexing
ExecStart=/usr/bin/dotnet /srv/services.stage/FilesIndexingService/FilesIndexingService.dll
Restart=always
RestartSec=10
KillSignal=SIGINT
SyslogIdentifier=dotnet-filesindexing-app.stage
User=www-data
Environment=DOTNET_ENVIRONMENT=Staging
Environment=DOTNET_PRINT_TELEMETRY_MESSAGE=false
Environment=TransmissionQueueNamePrefix=_stage

[Install]
WantedBy=multi-user.target