{
  "ProductEndpoint": "http://helpdeskservice",
  "ProductEndpointHttps": "https://helpdeskservice",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RabbitMQConfig": {
    "HostName": "localhost",
    "Port": 5672,
    "ClientProvidedName": "debug",
    "RemoteCallTimeoutMs": 3600000
  },
  "HelpDeskConfig": {
    "ConsoleSegmentCacheLifetimeSeconds": 604800
  },
  "ProxyNetMQConfig": {
    "PublisherSocketEndpoint": {
      "Scheme": "tcp",
      "Host": "127.0.0.1",
      "Port": 2883
    },
    "SubscriberSocketEndpoint": {
      "Scheme": "tcp",
      "Host": "127.0.0.1",
      "Port": 2884
    }
  },
  "RealtimeMQTTClientConfig": {
    "RemoteCallTimeoutMs": 3600000,
    "QueueMqNamePrefixForResponse": "realtime.bus-",
    "Scheme": "http",
    "Host": "localhost",
    "Port": 1883
  }
}