  "ConnectionStrings": {
    "RedisConnectionString": "localhost,defaultDatabase=0",
    "RedisConnectionString_stage": "localhost,defaultDatabase=1"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RabbitMQConfig": {
    "HostName": "localhost",
    "Port": 5672,
    "ClientProvidedName": "debug",
    "RemoteCallTimeoutMs": 3600000
  },
  "PartUploadSessionConfig": {
    "PartUploadSessionTimeoutSeconds": 20,
    "PartUploadSize": 2000000
  },
  "ApiAccess": {
    "TokenAccessHeaderName": "token-access"
  },
  "RealtimeMQTTClientConfig": {
    "RemoteCallTimeoutMs": 1600000,
    "QueueMqNamePrefixForResponse": "realtime.bus-",
    "Scheme": "http",
    "Host": "localhost",
    "Port": 1883
  },
  "NetMQProxyConfig": {
    "PublisherSocketEndpoint": {
      "Scheme": "tcp",
      "Host": "127.0.0.1",
      "Port": 2883
    },
    "SubscriberSocketEndpoint": {
      "Scheme": "tcp",
      "Host": "127.0.0.1",
      "Port": 2884
    }
  }
}